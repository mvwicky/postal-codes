{
  "tasks": {
    "dev": "deno run -A --watch main.ts",
    "run": "deno run -A main.ts",
    "cli": "deno run -A cli.ts"
  },
  "imports": {
    "std/": "https://deno.land/std@0.204.0/",
    "@/": "./"
  },
  "lint": {
